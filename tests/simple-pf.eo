
(declare-parameterized-const = ((T Type)) (-> T T Bool))


(declare-rule eq-symm ((T Type) (x T) (y T))
  :premises ((= T x y))
  :args ()
  :conclusion (= T y x))

  
(declare-const Int Type)
(declare-const a Int)
(declare-const b Int)
(declare-const c Bool)
(assume a1 (= Int a b))
(step a2 (= Int b a) :rule eq-symm :premises (a1) :args ())

